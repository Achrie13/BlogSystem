                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="博客">
                    <items>
                        <MenuItem mnemonicParsing="false" text="查询"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="帮助"/>
            </menus>
        </MenuBar>
        <ListView fx:id="BlogView" layoutY="27.0" prefHeight="773.0" prefWidth="1000.0"/>
    </children>
</AnchorPane>
